{
  "name": "angular2-webpack-starter-kit",
  "version": "1.0.0",
  "description": "Starter kit for Angular 2 and Webpack",
  "scripts": {
    "start": "npm-run-all --parallel open:src lint:watch",
    "open:src": "node tools/srcServer.js",
    "lint": "tslint ./src/**/*.ts -t verbose",
    "lint:watch": "npm run lint -- --watch",
    "test": "karma start karma.webpack.conf.js",
    "test:watch": "npm run test -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "build:html": "node tools/buildHtml.js",
    "prebuild": "npm-run-all clean-dist lint build:html",
    "build": "node tools/build.js",
    "postbuild": "node tools/distServer.js"
  },
  "keywords": [],
  "author": "Deborah Kurata & James Capps",
  "license": "ISC",
  "dependencies": {
    "@angular/common": "2.0.0",
    "@angular/compiler": "2.0.0",
    "@angular/core": "2.0.0",
    "@angular/forms": "2.0.0",
    "@angular/http": "2.0.0",
    "@angular/platform-browser": "2.0.0",
    "@angular/platform-browser-dynamic": "2.0.0",
    "@angular/router": "3.0.0",
    "core-js": "^2.4.1",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "^0.6.23",
    "bootstrap": "^3.3.6"
  },
  "devDependencies": {
    "@types/jasmine": "2.5.36",
    "@types/node": "^6.0.45",
    "angular2-template-loader": "^0.6.0",
    "awesome-typescript-loader": "^3.0.4",
    "cheerio": "^0.22.0",
    "css-loader": "^0.26.1",
    "express": "4.13.4",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "2.26.0",
    "jasmine": "~2.4.1",
    "jasmine-core": "~2.4.1",
    "jquery": "^3.2.1",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.1",
    "lodash": "^4.16.2",
    "node-sass": "^4.5.2",
    "npm-run-all": "1.8.0",
    "null-loader": "^0.1.1",
    "open": "0.0.5",
    "phantomjs-prebuilt": "^2.1.7",
    "protractor": "~4.0.14",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.4",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.13.1",
    "toastr": "^2.1.2",
    "tslint": "^3.15.1",
    "typescript": "^2.2.2",
    "webpack": "^2.3.2",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.17.1",
    "webpack-merge": "^4.1.0",
    "youtube-player": "^4.2.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jcapps/angular2-webpack-starter-kit"
  }
}
